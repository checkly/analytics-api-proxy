{
  "name": "analytics-api-proxy",
  "scope": "checkly",
  "build": {
    "env": {}
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Host", "value": "api.segment.io" }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "https://api.segment.com/:path*"
    }
  ]
}
